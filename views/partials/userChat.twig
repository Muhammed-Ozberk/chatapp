    {# <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script> #}
	<script src="/socket.io/socket.io.js"></script>

	<script>
		var socket = io();
        socket.on("connect", () => {
            console.log( "connected" )
        });

        var messageInput = document.getElementById( "messageInput" ); 
        var messagesList = document.getElementById( "messagesList" );

        document.getElementById( "submitButton" ).onclick =submitMessage;


        function submitMessage() {

            let item = document.createElement('li');
            let att = document.createAttribute("class");
            var localTime = new Date().toLocaleTimeString();

            att.value = "right";
            item.setAttributeNode(att);
            item.innerHTML="";
            item.innerHTML+=`
                <div class="conversation-list">
                  <div class="chat-avatar">
                        <div class="avatar-xs">
                            <span class="avatar-title rounded-circle bg-soft-primary text-primary ">
                                {{data.userAvatar}}
                            </span>
                        </div>
                  </div>
                  <div class="user-chat-content">
                      <div class="ctext-wrap">
                          <div class="ctext-wrap-content">
                              <p class="mb-0">
                                  ${messageInput.value}
                              </p>
                              <p class="chat-time mb-0"><i class="ri-time-line align-middle"></i> <span class="align-middle">${localTime}</span></p>
                          </div>
                              
                          <div class="dropdown align-self-start">
                              <a class="dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                  <i class="ri-more-2-fill"></i>
                              </a>
                              <div class="dropdown-menu">
                                  <a class="dropdown-item" href="#">Copy <i class="ri-file-copy-line float-end text-muted"></i></a>
                                  <a class="dropdown-item" href="#">Save <i class="ri-save-line float-end text-muted"></i></a>
                                  <a class="dropdown-item" href="#">Forward <i class="ri-chat-forward-line float-end text-muted"></i></a>
                                  <a class="dropdown-item" href="#">Delete <i class="ri-delete-bin-line float-end text-muted"></i></a>
                              </div>
                          </div>
                      </div>
                      
                      <div class="conversation-name">{{data.username}}</div>
                  </div>
            </div>`

        messagesList.appendChild(item);
        sendMessage(messageInput.value);
        messageInput.value="";
    };

        async function sendMessage(message){

          await saveMessage('{{data.roomID}}', message,'{{data.recipientID}}');
          socket.emit('sendMessage', message,'{{data.roomID}}');
        }
        
        socket.on('{{data.roomID}}', (msg) => {
        
          let item = document.createElement('li');
          var localTime = new Date().toLocaleTimeString();
        
          item.innerHTML="";
          item.innerHTML+=`
              <div class="conversation-list">
                  <div class="chat-avatar">
                    <div class="avatar-xs">
                        <span class="avatar-title rounded-circle bg-soft-primary text-primary ">
                            {{data.recipientAvatar}}
                        </span>
                    </div>
                </div>
                  <div class="user-chat-content">
                      <div class="ctext-wrap">
                          <div class="ctext-wrap-content">
                              <p class="mb-0">
                                ${ msg }
                              </p>
                              <p class="chat-time mb-0"><i class="ri-time-line align-middle"></i> <span class="align-middle">${localTime}</span></p>
                          </div>
                          <div class="dropdown align-self-start">
                              <a class="dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                  <i class="ri-more-2-fill"></i>
                              </a>
                              <div class="dropdown-menu">
                                  <a class="dropdown-item" href="#">Copy <i class="ri-file-copy-line float-end text-muted"></i></a>
                                  <a class="dropdown-item" href="#">Save <i class="ri-save-line float-end text-muted"></i></a>
                                  <a class="dropdown-item" href="#">Forward <i class="ri-chat-forward-line float-end text-muted"></i></a>
                                  <a class="dropdown-item" href="#">Delete <i class="ri-delete-bin-line float-end text-muted"></i></a>
                              </div>
                          </div>
                      </div>
                      <div class="conversation-name">{{data.recipientName}}</div>
                  </div>
              </div>`
            messagesList.appendChild(item);
            {# userChat.scrollTo(0, 300); #}
            readMessage('{{data.roomID}}','{{data.recipientID}}');

        });

        async function saveMessage(room,message,recipientID) {
            await $.ajax({
            method: "POST",
            url: "/message/save",
            dataType: 'json',
            data: {room, message,recipientID},
          }).done((res) => {
                console.log(res.status);
            }).fail((err)=>{
                console.log(err.status);
            });
        }
        
        function readMessage(roomID,recipientID) {
            $.ajax({
            method: "POST",
            url: "/chats/read",
            dataType: 'json',
            data: {roomID,recipientID},
          }).done((res) => {
                console.log(res.status);
            }).fail((err)=>{
                console.log(err.status);
            });
        }
            var quantityArray = [];
        
        	{% for row in data._chatList %}
                {% if data.roomID != row.room %}
                quantityArray.push({room:'{{row.room}}',quantity:0});
                socket.on('{{row.room}}', (msg) => {
                    var messagesQuantity = document.getElementById("{{row.room}}");
        			var lastMessage = document.getElementById("{{row.username}}");
        			let numb = messagesQuantity.childNodes.length;
        			if(numb != 0){
        				messagesQuantity.removeChild(messagesQuantity.firstElementChild);
        			}
                    let item = document.createElement('span');
                    let att = document.createAttribute("class");
        
        			lastMessage.innerHTML="";
        			lastMessage.innerHTML+=`${msg}`;
                    att.value = "badge badge-soft-danger rounded-pill";
                    item.setAttributeNode(att);
                    const found = quantityArray.find(element => element.room == '{{row.room}}');
                    const index = quantityArray.indexOf(found);
                    quantityArray[index].quantity++; 
                    item.innerHTML="";
                    item.innerHTML+=`${found.quantity}`
                    messagesQuantity.appendChild(item);
        
                });
                {% endif %}
        
        	{% endfor %}
        
        socket.on("connect_error", (err) => { console.log(`connect_error due to ${err.message}`); });
        
        socket.on( "disconnect", () => {
            console.log("logout")
        });
        	</script>

{% endblock %}


